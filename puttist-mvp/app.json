{
  "expo": {
    "name": "퍼티스트",
    "slug": "puttist",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "userInterfaceStyle": "dark",
    "newArchEnabled": true,
    "splash": {
      "image": "./assets/splash-icon.png",
      "resizeMode": "contain",
      "backgroundColor": "#0D1B0F"
    },
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.puttist.app"
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#0D1B0F"
      },
      "edgeToEdgeEnabled": true,
      "predictiveBackGestureEnabled": false,
      "package": "com.puttist.app"
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "plugins": [
      "expo-font",
      [
        "expo-camera",
        {
          "cameraPermission": "퍼팅 라인 인식을 위해 카메라 권한이 필요합니다."
        }
      ]
    ],
    "extra": {
      "eas": {
        "projectId": "0880691a-fabe-4c55-b028-be22c94e5ba8"
      }
    },
    "owner": "seiyeolo"
  }
}
